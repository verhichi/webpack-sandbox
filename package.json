{
  "name": "webpack-sandbox",
  "private": true,
  "version": "0.0.0",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "start:front": "npm run -w @webpack-sandbox/front dev",
    "build:front": "npm run -w @webpack-sandbox/front build",
    "preview:front": "npm run -w @webpack-sandbox/front preview",
    "start:server": "npm run -w @webpack-sandbox/server start:dev",
    "prepare": "husky install",
    "lint": "npm run -ws lint --if-present",
    "lint:fix": "npm run -ws lint:fix --if-present"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.3.0",
    "eslint-plugin-promise": "^6.1.0",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.1",
    "jest": "^29.2.0",
    "prettier": "^2.7.1",
    "typescript": "^4.8.4"
  }
}
